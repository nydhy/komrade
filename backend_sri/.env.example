# VetBridge Backend Environment
# Copy to .env and adjust values

# App
APP_NAME=VetBridge API
DEBUG=false

# Database (PostgreSQL 18 - port 5433)
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/vetbridge

# JWT (use: openssl rand -hex 32)
JWT_SECRET=change-me-in-production-use-openssl-rand-hex-32
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=10080
